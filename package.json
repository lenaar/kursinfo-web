{
  "//": "JSHint configuration - http://jshint.com/docs/options/",
  "name": "node-web",
  "version": "2.0.0",
  "description": "Template project for a node express web server",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "https://github.com/KTH/node-web"
  },
  "scripts": {
    "//": "Gulp installs css and javascript",
    "test": "mocha test/unit/*.js",
    "codecheck": "standard",
    "codecheck-fix": "standard --fix",
    "docker": "npm install --development && npm run build && npm prune --production",
    "build": "NODE_ENV=production gulp build",
    "start": "NODE_ENV=production node app.js",
    "start-dev": "cross-env NODE_ENV=development gulp build && cross-env NODE_ENV=development concurrently --kill-others \"nodemon --ignore dist/**/* app.js\" \"gulp watch\"",
    "watch-dev": "cross-env NODE_ENV=development gulp watch\""
  },
  "pre-push": [
    "codecheck",
    "test"
  ],
  "dependencies": {
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.1",
    "bootstrap": "^3.3.6",
    "co": "^4.6.0",
    "component-registry": "^0.2.0",
    "cookie-parser": "^1.4.2",
    "dotenv": "^4.0.0",
    "es5-shim": "^4.5.9",
    "es6-shim": "^0.35.3",
    "express": "^4.13.4",
    "express-handlebars": "^3.0.0",
    "history": "^4.7.2",
    "html-entities": "^1.2.1",
    "inferno": "^3.10.1",
    "inferno-animation": "^0.4.7",
    "inferno-bootstrap": "^0.2.6",
    "inferno-component": "^3.10.1",
    "inferno-formlib": "^0.2.19",
    "inferno-mobx": "^3.10.1",
    "inferno-router": "^3.10.1",
    "inferno-server": "^3.10.1",
    "isomorphic-schema": "^0.7.1",
    "jquery": "^3.1.0",
    "kth-client-logging": "https://github.com/KTH/kth-client-logging.git#v1.0.0",
    "kth-node-access-log": "^0.2.0",
    "kth-node-api-call": "^1.2.2",
    "kth-node-configuration": "^1.4.0",
    "kth-node-express-routing": "^0.3.1",
    "kth-node-i18n": "^1.0.2",
    "kth-node-ldap": "^2.0.1",
    "kth-node-log": "^3.0.0",
    "kth-node-monitor": "^0.1.5",
    "kth-node-passport-cas": "^3.0.2",
    "kth-node-redis": "^1.2.0",
    "kth-node-server": "^3.0.3",
    "kth-node-session": "^1.0.0",
    "kth-node-web-common": "^0.1.1",
    "kth-style": "^2.0.0-beta.1",
    "kth-style-inferno-bootstrap": "^0.1.15",
    "locale": "^0.1.0",
    "mobx": "^3.3.1",
    "passport": "^0.3.2",
    "safe-utils": "^0.1.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.0.2",
    "concurrently": "^2.1.0",
    "cross-env": "^2.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "gulp": "^3.9.1",
    "husky": "^0.14.0",
    "inferno-devtools": "^3.10.1",
    "kth-node-build-commons": "^3.0.0",
    "kth-node-inferno": "^0.2.0",
    "merge-stream": "^1.0.1",
    "mocha": "3.2.0",
    "mockery": "^2.0.0",
    "nock": "^9.0.13",
    "node-mocks-http": "^1.6.3",
    "nodemon": "^1.11.0",
    "pre-push": "^0.1.1",
    "standard": "^7.1.1",
    "zombie": "5.0.5"
  },
  "engines": {
    "node": "8.9.1"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "standard-jsx"
    ],
    "ignore": [
      "public/js/lib"
    ]
  }
}
